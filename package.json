{
  "name": "mwasm",
  "version": "0.0.2",
  "description": "wasmテキストフォーマットにマクロ機能を追加するプリプロセッサ",
  "main": "index.js",
  "bin": "bin/mwasm",
  "scripts": {
    "build":
    "pegjs -o ./lib/mwasm-parser.js ./lib/mwasm.pegjs && rollup -c ",
    "test": "pegjs -o ./lib/mwasm-parser.js ./lib/mwasm.pegjs && rollup -c && ./bin/mwasm ./examples/psg-emulator/em2149.mwat -o ./examples/psg-emulator/em2149.wasm",
    "trace": "pegjs --trace -o ./lib/mwasm-parser.js ./lib/mwasm.pegjs && rollup -c && ./bin/mwasm ./examples/psg-emulator/em2149.mwat -o ./examples/psg-emulator/em2149.wasm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sfpgmr/mwasm.git"
  },
  "keywords": [
    "wasm"
  ],
  "author": "S.F.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sfpgmr/mwasm/issues"
  },
  "homepage": "https://github.com/sfpgmr/mwasm#readme",
  "devDependencies": {
    "pegjs": "^0.10.0",
    "rollup": "^1.10.1",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3"
  },
  "dependencies": {
    "wabt": "^1.0.10"
  }
}
